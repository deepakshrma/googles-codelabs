    "http-server": "^0.12.1",
    "lint-staged": "^10.1.7",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5"
  },
  "pre-commit": [
    "pre-commit"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write"
